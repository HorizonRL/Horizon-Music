    "version": "0.2.0",
    "configurations": [
        {
            "name": "Horizon Music",
            "type": "python",
            "request": "launch",
            "program": "${workspaceRoot}/src/HorizonMusic.py",
            "console": "integratedTerminal",
            "pythonPath": "${config:python.interpreterPath}",
            "cwd": "${workspaceRoot}",
            "env": {
                "PYTHONPATH": "${workspaceRoot}"
            }
        },
        {
            "name": "Server",
            "type": "python",
            "request": "launch",
            "program": "${workspaceRoot}/src/Server.py",
            "console": "integratedTerminal",
            "pythonPath": "${config:python.interpreterPath}",
            "cwd": "${workspaceRoot}",
            "env": {
                "PYTHONPATH": "${workspaceRoot}"
            }
        }
    ]
}